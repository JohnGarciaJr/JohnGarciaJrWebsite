{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "disown-opener": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-ms-touch-action",
          "scrollbar-width",
          "scroll-snap-stop"
        ]
      }
    ],
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/layout": "off",
    "detect-css-reflows/paint": "off"
  }
}